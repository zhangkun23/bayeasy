/* pages/invoice/incomeInvoice/downloadPage/index/index.wxss */

.downloadPage {
  height: 100vh;
  overflow: hidden;
}

.navigationBar {
  /* color: #fff; */
}

.downloadPage__container {
  flex: 1;
  height: 1rpx;
  margin: 32rpx;
  overflow-y: auto;
}

.detail__topTips {
  line-height: 36rpx;
  border-radius: 12rpx;
  padding: 16rpx 22rpx;
  margin-bottom: 32rpx;
}

.detail__topTips-icon {
  align-items: center;
}

.detail__topTips-icon image {
  width: 24rpx;
  height: 24rpx;
  margin-right: 16rpx;
}

.downloadPage__header {
  margin-bottom: 16rpx;
}

.downloadPage__header-image {
  width: 44rpx;
  height: 44rpx;
  margin-right: 12rpx;
}

.downloadPage__tips {
  font-size: 20rpx;
  line-height: 32rpx;
}