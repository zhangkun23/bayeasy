

.bg{
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(0, 0, 0, 0.3);
    z-index: 3;
}
.count{
    position: relative;
    margin: 90rpx 32rpx;
    background: #F3F8FC;
    box-shadow: 0px 10rpx 30rpx 0rpx rgba(201, 209, 228, 0.38);
    border-radius: 28rpx;
    padding: 32rpx 38rpx;
}
.title{
    display: flex;
    justify-content: space-between;
}
.titleBottom{
    margin-bottom: 36rpx;
}
.title>view{
    font-size: 32rpx;
    font-weight: 600;
    color: #283F56;
}
.title>image{
    width: 40rpx;
    height: 40rpx;   
}
.err__item{
    padding: 24rpx;
    padding-bottom: 0rpx;
    height: 196rpx;
    background: #FFFFFF;
    border-radius: 16rpx;
}
.gsName{
    padding-top: 16rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: #6E87A2;
}
.err__info{
    margin: 16rpx 0rpx;
    height: 44rpx;
    line-height: 44rpx;
    padding-left: 20rpx;
    background: #FFEBF1;
    border-radius: 12rpx;
    font-size: 20rpx;
    font-weight: 400;
    color: #FF475A;
    display: flex;
    align-items: center;
}
.err__info>image{
    height: 24rpx;
    width: 24rpx;
    padding-right: 10rpx;
}