/**app.wxss**/
@import '/assets/index.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.bgColog {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: -1;
  background-image: url('https://image.bayeasy.cn/images-data/public/backGround.png');
  background-position: top;
  background-repeat: no-repeat;
  background-size: contain;
}